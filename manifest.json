{
  "manifest_version": 2,
  "name": "Leap Motion DIRECTV Controller",
  "description": "Chrome extension for controlling Directv set top box with Leap Motion.",
  "version": "0.1",
  "background": {
    "scripts": [
    	"public/js/vendor/leap.min.js",
    	"public/js/core/background.js"
    ]
  },
  "icons": {"128": "public/img/icon128.png"},
  "browser_action": {
     "default_popup": "public/index.html",
     "default_title": "LeapDebug",
     "default_icon": {
     	"19": "public/img/icon19_inactive.png",
     	"38": "public/img/icon38_inactive.png"
  	}
  },
  "permissions": [
  	 "storage"
  ]
}
